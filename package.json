  },
  "dependencies": {
    "core-js": "^3.2.1",
    "json-to-ast": "^2.0.3",
    "jsonpointer": "^4.0.1",
    "leven": "^3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "7.6.0",
    "@babel/plugin-syntax-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "7.0.0",
    "@changesets/cli": "^1.3.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.9.0",
    "babel-plugin-add-module-exports": "^1.0.0",
    "dotenv": "^8.1.0",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.2.0",
    "eslint-plugin-jest": "^22.17.0",
    "eslint-plugin-prettier": "^3.1.0",
    "flow-bin": "^0.107.0",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "jest-fixtures": "^0.6.0",
    "prettier": "^1.14.2",
    "pretty-quick": "^1.6.0",
    "svg-term-cli": "^2.1.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "peerDependencies": {
    "ajv": "4.11.8 - 6"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js"
    ]
  }
