{
  "name": "@sidvind/better-ajv-errors",
  "version": "0.9.0",
  "description": "JSON Schema validation for Human",
  "repository": "ext/better-ajv-errors",
  "main": "index.js",
  "keywords": [
    "json-schema",
    "ajv",
    "ajv-errors"
  ],
  "author": "Rifat Nabi <to.rifat@gmail.com>",
  "maintainers": [
    "Rifat Nabi <to.rifat@gmail.com>",
    "Dmitrii Sorin <info@staypositive.ru>",
    "Tong Li"
  ],
  "license": "Apache-2.0",
  "typings": "typings.d.ts",
  "engines": {
    "node": ">= 10.0"
  },
  "files": [
    "dist",
    "index.js",
    "typings.d.ts",
    "!**/__tests__",
    "!dist/test-helpers.js"
  ],
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "babel src -d dist/",
    "prepare": "husky install",
    "prerelease": "yarn build",
    "release": "changeset release",
    "format": "prettier --write src .changeset",
    "test": "jest",
    "test-ci": "jest --coverage --colors",
    "screenshot": "svg-term --command='node screenshot' --out=media/screenshot.svg --padding=5 --width=80 --height=13 --at=1000 --no-cursor --term iterm2 --profile='deep' --window"
  },
  "dependencies": {
    "@babel/code-frame": "^7.0.0",
    "chalk": "^4.0.0",
    "json-to-ast": "^2.0.3",
    "jsonpointer": "^4.1.0",
    "leven": "^3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "7.14.3",
    "@babel/core": "7.14.3",
    "@babel/preset-env": "7.14.2",
    "@changesets/cli": "1.3.3",
    "ajv": "8.5.0",
    "ajv4": "npm:ajv@4.11.8",
    "ajv5": "npm:ajv@5.0.0",
    "ajv6": "npm:ajv@6.0.0",
    "ajv7": "npm:ajv@7.0.0",
    "ajv8": "npm:ajv@8.0.0",
    "babel-jest": "26.6.3",
    "eslint": "7.26.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-jest": "24.3.6",
    "eslint-plugin-prettier": "3.4.0",
    "flow-bin": "0.107.0",
    "husky": "6.0.0",
    "jest": "26.6.3",
    "jest-fixtures": "0.6.0",
    "prettier": "2.3.0",
    "pretty-quick": "3.1.0",
    "svg-term-cli": "2.1.1"
  },
  "peerDependencies": {
    "ajv": "4.11.8 - 8"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js"
    ]
  },
  "renovate": {
    "extends": [
      "@html-validate",
      "@html-validate:github"
    ]
  }
}
