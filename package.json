{
  "name": "better-ajv-errors",
  "version": "0.8.2",
  "description": "JSON Schema validation for Human",
  "repository": "atlassian/better-ajv-errors",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "keywords": [
    "json-schema",
    "ajv",
    "ajv-errors"
  ],
  "author": "Rifat Nabi <to.rifat@gmail.com>",
  "maintainers": [
    "Rifat Nabi <to.rifat@gmail.com>",
    "Dmitrii Sorin <info@staypositive.ru>",
    "Tong Li"
  ],
  "license": "Apache-2.0",
  "typings": "typings.d.ts",
  "files": [
    "lib",
    "index.js",
    "typings.d.ts"
  ],
  "scripts": {
    "prebuild": "rm -rf lib",
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "node build.js --cjs",
    "build:esm": "node build.js",
    "prerelease": "npm run build",
    "release": "changeset release",
    "format": "prettier --write src/*.js src/**/*.js .changeset/*.js",
    "test": "jest",
    "test-ci": "jest --coverage --colors",
    "screenshot": "svg-term --command='node screenshot' --out=media/screenshot.svg --padding=5 --width=80 --height=13 --at=1000 --no-cursor --term iterm2 --profile='deep' --window"
  },
  "dependencies": {
    "@babel/code-frame": "^7.16.0",
    "chalk": "^2.4.1",
    "core-js": "^3.19.1",
    "json-to-ast": "^2.0.3",
    "jsonpointer": "^5.0.0",
    "leven": "^3.1.0"
  },
  "devDependencies": {
    "@changesets/cli": "^1.3.0",
    "ajv": "^8.6.3",
    "esbuild": "^0.13.12",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^25.2.3",
    "eslint-plugin-prettier": "^4.0.0",
    "fast-glob": "^3.2.7",
    "flow-bin": "^0.163.0",
    "husky": "^7.0.4",
    "is-ci": "^3.0.1",
    "jest": "^27.3.1",
    "jest-fixtures": "^0.6.0",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1",
    "svg-term-cli": "^2.1.1"
  },
  "peerDependencies": {
    "ajv": "4.11.8 - 8"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js"
    ]
  }
}
