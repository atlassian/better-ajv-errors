{
  "name": "@sidvind/better-ajv-errors",
  "version": "0.8.0",
  "description": "JSON Schema validation for Human",
  "repository": "ext/better-ajv-errors",
  "main": "index.js",
  "keywords": [
    "json-schema",
    "ajv",
    "ajv-errors"
  ],
  "author": "Rifat Nabi <to.rifat@gmail.com>",
  "maintainers": [
    "Rifat Nabi <to.rifat@gmail.com>",
    "Dmitrii Sorin <info@staypositive.ru>",
    "Tong Li"
  ],
  "license": "Apache-2.0",
  "typings": "typings.d.ts",
  "engines": {
    "node": ">= 10.0"
  },
  "files": [
    "dist",
    "index.js",
    "typings.d.ts",
    "!**/__tests__",
    "!dist/test-helpers.js"
  ],
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "babel src -d dist/",
    "prerelease": "yarn build",
    "release": "changeset release",
    "format": "prettier --write src/*.js src/**/*.js .changeset/*.js",
    "test": "jest",
    "test-ci": "jest --coverage --colors",
    "screenshot": "svg-term --command='node screenshot' --out=media/screenshot.svg --padding=5 --width=80 --height=13 --at=1000 --no-cursor --term iterm2 --profile='deep' --window"
  },
  "dependencies": {
    "@babel/code-frame": "^7.0.0",
    "chalk": "^4.0.0",
    "json-to-ast": "^2.0.3",
    "jsonpointer": "^4.0.1",
    "leven": "^3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "7.12.10",
    "@babel/core": "7.12.10",
    "@babel/preset-env": "7.12.11",
    "@changesets/cli": "1.3.3",
    "babel-jest": "26.6.3",
    "eslint": "7.18.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-prettier": "3.3.1",
    "flow-bin": "0.107.0",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "jest-fixtures": "0.6.0",
    "prettier": "2.2.1",
    "pretty-quick": "3.1.0",
    "svg-term-cli": "2.1.1"
  },
  "peerDependencies": {
    "ajv": "4.11.8 - 7"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js"
    ]
  },
  "renovate": {
    "extends": [
      "@html-validate",
      "@html-validate:github"
    ]
  }
}