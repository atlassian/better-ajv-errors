{
  "name": "@sidvind/better-ajv-errors",
  "version": "0.9.1",
  "description": "JSON Schema validation for Human",
  "repository": "https://github.com/ext/better-ajv-errors.git",
  "homepage": "https://github.com/ext/better-ajv-errors",
  "bugs": "https://github.com/ext/better-ajv-errors/issues",
  "sideEffects": false,
  "main": "index.js",
  "keywords": [
    "json-schema",
    "ajv",
    "ajv-errors"
  ],
  "author": "Rifat Nabi <to.rifat@gmail.com>",
  "maintainers": [
    "Rifat Nabi <to.rifat@gmail.com>",
    "Dmitrii Sorin <info@staypositive.ru>",
    "Tong Li"
  ],
  "license": "Apache-2.0",
  "typings": "typings.d.ts",
  "engines": {
    "node": ">= 12.0"
  },
  "files": [
    "dist",
    "index.js",
    "typings.d.ts",
    "!**/__tests__",
    "!dist/test-helpers.js"
  ],
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "babel src -d dist/",
    "prepare": "husky install",
    "prerelease": "npm run build",
    "eslint": "eslint src",
    "release": "changeset release",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "test": "jest --colors",
    "screenshot": "svg-term --command='node screenshot' --out=media/screenshot.svg --padding=5 --width=80 --height=13 --at=1000 --no-cursor --term iterm2 --profile='deep' --window"
  },
  "dependencies": {
    "@babel/code-frame": "^7.0.0",
    "chalk": "^4.0.0",
    "json-to-ast": "^2.0.3",
    "jsonpointer": "^4.1.0",
    "leven": "^3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "7.14.8",
    "@babel/core": "7.14.8",
    "@babel/preset-env": "7.14.8",
    "@changesets/cli": "1.3.3",
    "ajv": "8.6.2",
    "ajv4": "npm:ajv@4.11.8",
    "ajv5": "npm:ajv@5.0.0",
    "ajv6": "npm:ajv@6.0.0",
    "ajv7": "npm:ajv@7.0.0",
    "ajv8": "npm:ajv@8.0.0",
    "babel-jest": "27.0.6",
    "eslint": "7.31.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-jest": "24.4.0",
    "eslint-plugin-prettier": "3.4.0",
    "flow-bin": "0.156.0",
    "husky": "7.0.1",
    "jest": "27.0.6",
    "jest-fixtures": "0.6.0",
    "prettier": "2.3.2",
    "pretty-format": "27.0.6",
    "pretty-quick": "3.1.1",
    "svg-term-cli": "2.1.1"
  },
  "peerDependencies": {
    "ajv": "4.11.8 - 8"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.[jt]s",
      "!**/__tests__/",
      "!**/*.d.ts",
      "!**/index.[jt]s"
    ],
    "coverageReporters": [
      "text",
      "text-summary",
      "html",
      "json",
      "lcov"
    ],
    "snapshotSerializers": [
      "pretty-format/build/plugins/ConvertAnsi"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/dist/"
    ]
  },
  "renovate": {
    "extends": [
      "@html-validate",
      "@html-validate:github"
    ],
    "ignoreDeps": [
      "ajv4",
      "ajv5",
      "ajv6",
      "ajv7",
      "ajv8"
    ]
  }
}